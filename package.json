{
  "name": "wiser-challenge-shortener",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "14.16.0"
  },
  "scripts": {
    "build": "node ace build --production",
    "postbuild": "NODE_ENV=production HOST=0.0.0.0 PORT=3333 APP_NAME=1 APP_KEY=SOME-SECURE-APP-KEY DB_CONNECTION=pg PG_HOST=0.0.0.0 PG_PORT=1 PG_USER=1 PG_PASSWORD=1 PG_DB_NAME=1 node ace swagger:generate && cp -a docs/ build/docs",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.2",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.2",
    "eslint-plugin-prettier": "^3.4.0",
    "pino-pretty": "^5.0.2",
    "prettier": "^2.3.1",
    "typescript": "^4.2.4",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.1.8",
    "@adonisjs/lucid": "^15.0.1",
    "@adonisjs/repl": "^3.1.4",
    "adonis5-swagger": "^1.3.3",
    "luxon": "^1.27.0",
    "pg": "^8.6.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
